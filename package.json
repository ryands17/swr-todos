{
  "name": "react-swr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.0",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "swr": "^0.1.18",
    "todomvc-app-css": "2.2.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "craco start",
    "start:server": "json-server --watch db.json -p 3001",
    "build": "craco build",
    "preprod": "npm run build",
    "prod": "concurrently \"npx serve -s build\" \"npm run start:server\"",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@types/concurrently": "^5.1.0",
    "@types/json-server": "^0.14.2",
    "@types/prettier": "^1.19.1",
    "concurrently": "^5.1.0",
    "craco-workbox": "^0.1.0",
    "json-server": "^0.16.1",
    "prettier": "^2.0.2",
    "typesync": "^0.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
