{
  "name": "swr-todos",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.1",
    "@material-ui/icons": "4.9.1",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "axios": "0.21.1",
    "clsx": "1.1.1",
    "immer": "8.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-query": "2.26.3",
    "react-scripts": "3.4.3",
    "todomvc-app-css": "2.3.0",
    "typescript": "4.1.3"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "prod": "run-p start:server run:client",
    "dev:client": "craco start",
    "dev:server": "json-server --watch db.json -p 3001",
    "build": "craco build",
    "postbuild": "cd build && cat sw.js >> service-worker.js",
    "start:client": "npx serve -s build",
    "start": "run-p start:client dev:server",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "cy:test": "dotenv -- cypress open",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.75%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "5.8.0",
    "@types/json-server": "0.14.2",
    "@types/prettier": "2.1.6",
    "craco-workbox": "0.1.0",
    "cypress": "5.4.0",
    "dotenv-cli": "4.0.0",
    "husky": "4.3.8",
    "json-server": "0.16.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "react-query-devtools": "2.6.3",
    "typesync": "0.8.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": "prettier --write"
  }
}
