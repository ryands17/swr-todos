{
  "name": "swr-todos",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@types/jest": "27.5.2",
    "@types/node": "17.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "axios": "0.27.2",
    "clsx": "1.1.1",
    "immer": "9.0.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.1",
    "react-scripts": "4.0.2",
    "todomvc-app-css": "2.4.2",
    "typescript": "4.6.4"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "prod": "run-p start:server run:client",
    "dev:client": "craco start",
    "dev:server": "json-server --watch db.json -p 3001",
    "build": "craco build",
    "postbuild": "cd build && cat sw.js >> service-worker.js",
    "start:client": "npx serve -s build",
    "start": "run-p start:client dev:server",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "cy:test": "dotenv -- cypress open",
    "eject": "react-scripts eject",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.75%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "6.4.0",
    "@types/json-server": "0.14.4",
    "@types/prettier": "2.6.3",
    "craco-workbox": "0.2.0",
    "cypress": "9.7.0",
    "dotenv-cli": "5.1.0",
    "husky": "7.0.4",
    "json-server": "0.17.0",
    "lint-staged": "12.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typesync": "0.9.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": "prettier --write"
  }
}
